#!/bin/sh
#
# This file has been autogenerated by LinuxSetup.
# Do not change it manually
#

(nitrogen --set-zoom-fill ~/Wallpapers/active ;  sleep 1 ;  nitrogen --set-zoom-fill ~/Wallpapers/active) &
xrandr --output Virtual-1 --mode 1920x1080 &
picom -b --no-fading-openclose &
dwmblocks &

while true; do
    dwm >/dev/null 2>&1 || break
done
